<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChatActivity">

  <ImageView
      android:id="@+id/imageView74"
      android:layout_width="230dp"
      android:layout_height="25dp"
      android:layout_marginTop="180dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.088"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/quickstring" />

  <ImageButton
      android:id="@+id/imageButton18"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="592dp"
      android:background="#00000000"
      android:contentDescription="Button for alert"

      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/alertbutton" />

  <ImageButton
      android:id="@+id/imageButton19"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="#00000000"
      android:contentDescription="Button for send"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.96"
      app:srcCompat="@drawable/sendbutton" />

  <EditText
      android:id="@+id/editTextTime4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="36dp"
      android:layout_marginEnd="24dp"
      android:contentDescription="Selector for time"
      android:ems="10"
      android:inputType="time"
      android:minHeight="48dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toEndOf="@+id/imageView13"
      app:layout_constraintTop_toBottomOf="@+id/imageButton109"
      tools:ignore="EditableContentDescCheck" />

  <ImageView
      android:id="@+id/imageView13"
      android:layout_width="170dp"
      android:layout_height="21dp"
      android:layout_marginTop="48dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageButton109"
      app:srcCompat="@drawable/selecttimestring" />

  <ImageView
      android:id="@+id/imageView8"
      android:layout_width="194dp"
      android:layout_height="21dp"
      android:layout_marginTop="76dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.073"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/recentcontactstring" />

  <Spinner
      android:id="@+id/spinner"
      android:layout_width="340dp"
      android:layout_height="48dp"
      android:contentDescription="dropdown for contact list"
      android:hint="Email"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.492"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView8" />

  <ImageButton
      android:id="@+id/imageButton15"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:background="F000000"
      android:contentDescription="Button for go back"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.023"
      app:srcCompat="@drawable/iconback" />

  <ImageButton
      android:id="@+id/imageButton17"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="16dp"
      android:background="F000000"
      android:contentDescription="Button for notifications"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.023"
      app:srcCompat="@drawable/iconnotification" />

  <ImageView
      android:id="@+id/imageView14"
      android:layout_width="151dp"
      android:layout_height="17dp"
      android:layout_marginTop="24dp"
      android:layout_marginEnd="52dp"
      app:layout_constraintBottom_toTopOf="@+id/imageView8"
      app:layout_constraintEnd_toStartOf="@+id/imageButton17"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.25"
      app:srcCompat="@drawable/chatstring" />

  <ImageButton
      android:id="@+id/imageButton109"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:layout_marginEnd="4dp"
      android:background="#00000000"
      android:contentDescription="Button for 1hr"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/imageButton117"
      app:layout_constraintTop_toBottomOf="@+id/imageView74"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/onehrbutton" />

  <ImageButton
      android:id="@+id/imageButton117"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:layout_marginEnd="12dp"
      android:background="#00000000"
      android:contentDescription="Button for 3hr"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView74"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/threehrbutton" />

  <ImageButton
      android:id="@+id/imageButton118"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:layout_marginEnd="8dp"
      android:background="#00000000"
      android:contentDescription="Button for 10min"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/imageButton109"
      app:layout_constraintTop_toBottomOf="@+id/imageView74"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/tenminbutton" />

  <ImageView
      android:id="@+id/imageView77"
      android:layout_width="185dp"
      android:layout_height="25dp"
      android:layout_marginTop="80dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.07"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView13"
      app:srcCompat="@drawable/leavestring" />

  <EditText
      android:id="@+id/editTextTextPersonName7"
      android:layout_width="367dp"
      android:layout_height="54dp"
      android:layout_marginTop="4dp"
      android:ems="10"
      android:inputType="textPersonName"
      android:text="Message"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView77" />

</androidx.constraintlayout.widget.ConstraintLayout>