<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChatActivity">

  <ImageButton
      android:id="@+id/imageButton139"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="196dp"
      android:background="#00000000"
      android:contentDescription="Button for other time"
      app:layout_constraintBottom_toTopOf="@+id/imageButton19"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.489"
      app:layout_constraintStart_toStartOf="parent"
      app:srcCompat="@drawable/timeanddatebutton" />

  <ImageButton
      android:id="@+id/imageButton19"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="#00000000"
      android:contentDescription="Button for send"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.881"
      app:srcCompat="@drawable/sendbutton" />

  <ImageView
      android:id="@+id/imageView13"
      android:layout_width="170dp"
      android:layout_height="21dp"
      android:layout_marginStart="4dp"
      android:layout_marginTop="220dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/selecttimestring" />

  <ImageView
      android:id="@+id/imageView8"
      android:layout_width="194dp"
      android:layout_height="21dp"
      android:layout_marginTop="96dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.073"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/recentcontactstring" />

  <Spinner
      android:id="@+id/spinner"
      android:layout_width="340dp"
      android:layout_height="48dp"
      android:layout_marginTop="16dp"
      android:contentDescription="dropdown for contact list"
      android:hint="Email"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.492"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView8" />

  <ImageButton
      android:id="@+id/imageButton15"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:background="F000000"
      android:contentDescription="Button for go back"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.023"
      app:srcCompat="@drawable/iconback" />

  <ImageButton
      android:id="@+id/imageButton17"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="16dp"
      android:background="F000000"
      android:contentDescription="Button for notifications"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.023"
      app:srcCompat="@drawable/iconnotification" />

  <ImageView
      android:id="@+id/imageView14"
      android:layout_width="151dp"
      android:layout_height="17dp"
      android:layout_marginTop="24dp"
      android:layout_marginEnd="52dp"
      app:layout_constraintBottom_toTopOf="@+id/imageView8"
      app:layout_constraintEnd_toStartOf="@+id/imageButton17"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.25"
      app:srcCompat="@drawable/chatstring" />

  <ImageButton
      android:id="@+id/imageButton109"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="4dp"
      android:layout_marginBottom="404dp"
      android:background="#00000000"
      android:contentDescription="Button for 1hr"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/imageButton117"
      app:layout_constraintTop_toBottomOf="@+id/imageView13"
      app:layout_constraintVertical_bias="0.941"
      app:srcCompat="@drawable/onehrbutton" />

  <ImageButton
      android:id="@+id/imageButton117"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="32dp"
      android:layout_marginEnd="12dp"
      android:background="#00000000"
      android:contentDescription="Button for 3hr"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView13"
      app:srcCompat="@drawable/threehrbutton" />

  <ImageButton
      android:id="@+id/imageButton118"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="4dp"
      android:layout_marginBottom="404dp"
      android:background="#00000000"
      android:contentDescription="Button for 10min"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/imageButton109"
      app:layout_constraintTop_toBottomOf="@+id/imageView13"
      app:layout_constraintVertical_bias="0.941"
      app:srcCompat="@drawable/tenminbutton" />

</androidx.constraintlayout.widget.ConstraintLayout>